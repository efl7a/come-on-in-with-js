<% if current_user.attending?(study_session) %>
  <%= button_to "Attending - Change Attendance", "", form_class: "student", class: 'btn btn-xs student', data: {attendee_id: current_user.attendance_record(study_session).id} %>
<% else %>
  <%= button_to "Attend", "",  form_class: "student", class: 'btn btn-xs' %>
<% end %>
